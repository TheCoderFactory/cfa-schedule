<h1>Listing event_times</h1>

<table class="table">
    <tr>
      <th>Event</th>
      <th>Date</th>
      <th>Start_time</th>
      <th>End_time</th>
        <th colspan="3"></th>
    </tr>

  <%= content_tag_for(:tr, @event_times) do |event_time| %>
    <td><%= event_time.event %></td>
    <td><%= event_time.date %></td>
    <td><%= event_time.start_time %></td>
    <td><%= event_time.end_time %></td>
      <td><%= link_to 'Show', event_time %></td>
    <td><%= link_to 'Edit', edit_event_time_path(event_time), class: 'btn btn-edit' %></td>
    <td><%= link_to 'Delete', event_time, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-destroy' %></td>
  <% end %>
</table>


<div class="form-actions">
  <%= link_to 'New Event time', new_event_time_path, class: 'btn btn-success' %>
</div>
